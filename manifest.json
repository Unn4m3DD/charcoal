{
  "manifest_version": 2,
  "name": "Charcoal Messenger",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://*.messenger.com/*"],
      "css": ["themes.css", "custom.css"],
      "js": ["init.js"]
    }
  ],
  "permissions": ["storage"],
  "web_accessible_resources": [
    "themes.css",
    "custom.css",
    "charcoal-messenger.svg",
    "facebook-messenger.svg"
  ]
}